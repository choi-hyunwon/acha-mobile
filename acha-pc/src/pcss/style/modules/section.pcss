                                & span {
                                    position: absolute;
                                    top: 23px;
                                    left: 50%;
                                    transform: translateX(-50%);
                                }
                            }

                            &.text {
                                position: relative;

                                & .title {
                                    position: absolute;
                                    font-family: var(--font2);
                                    font-size: 15px;
                                    font-weight: 500;
                                    line-height: 22px;
                                    letter-spacing: -0.38px;
                                    color: var(--text-02);
                                    top: 24px;
                                    left: 24px;
                                }

                                & .item {
                                    position: relative;
                                    display: block;

                                    & .num {
                                        display: inline-block;
                                        position: absolute;
                                    }

                                    & .desc {
                                        display: block;
                                        position: absolute;
                                        font-size: 15px;
                                        line-height: 25px;
                                        color: var(--text-02);
                                    }
                                }
                            }
                        }

                        &:last-child {
                            & td {
                                border-bottom: 1px solid #e3e4e8;
                            }
                        }
                    }
                }
            }

            & .t-table {
                margin-bottom: 150px;

                & .row {
                    padding: 0;
                    margin: 0;

                    &:first-child {
                        border-top: 1px solid #e3e4e8;

                        & .col {
                            padding: 0;
                            margin: 0;
                            flex-basis: initial;

                            & > span {
                                display: block;
                                height: 52px;
                                background-color: #f5f6f8;
                                font-family: var(--font2);
                                line-height: 50px;
                                font-size: 15px;
                                font-weight: 500;
                                letter-spacing: -0.38px;
