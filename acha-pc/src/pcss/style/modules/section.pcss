                    }
                }

                & tbody {
                    & tr {
                        & td {
                            font-size: 15px;
                            line-height: 50px;
                            letter-spacing: -0.38px;
                            text-align: center;
                            color: var(--text-02);
                            border-right: 1px solid #e3e4e8;
                            border-bottom: 1px solid #f5f6f8;

                            &:last-child {
                                border-right: 0;
                            }

