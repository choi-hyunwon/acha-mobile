/*
 section_expense ----------------------------------------
*/
.section_expense {
    position: relative;
    height: 864px;

    & .inner {
        padding-top: 80px;

        & .txt-area {
            text-align: center;
            margin-bottom: 60px;

            & .txt-sm {
                font-size: 18px;
                font-weight: bold;
                line-height: 27px;
                letter-spacing: normal;
                color: var(--text-point-color);
                margin-bottom: 3px;
            }

            & .txt-lg {
                font-size: 28px;
                font-family: var(--font2);
                line-height: 37px;
                text-align: center;
                margin-bottom: 8px;
            }

            & .desc {
                font-family: var(--font4);
                font-size: 15px;
                font-weight: 300;
                line-height: 21px;
                letter-spacing: -0.38px;
                color: var(--text-02);
                margin-bottom: 0;
                margin-top: 0;
            }
        }

        & .wrap_tabs {
            position: relative;
            width: 100%;
            height: 683px;
            margin-bottom: 15px;

            & .panel {
                position: relative;
                z-index: 1;

                & .img {
                    width: 320px;
                    height: auto;
                    margin: 0 auto;

                    & img {
                        width: 100%;
                        height: 100%;
                    }
                }

                &:last-of-type {
                    & .img {
                        display: block;
                        width: 320px;
                        height: auto;
                    }

                    &::before {
                        content: none;
                    }
                }

                &::before {
                    content: '';
                    display: block;
                    position: absolute;
                    width: 320px;
                    height: 370px;
                    top: 0;
                    left: 50%;
                    transform: translateX(-50%);
                    background-image: url('../../img/main/img_phone_02_m.png');
                    z-index: -1;
                }

                &.panel-sm {
                    padding-top: 124px;
                }
            }

            & .tabs {
                position: absolute;
                width: 320px;
                bottom: 179px;
                left: 50%;
                transform: translateX(-50%);
                z-index: 5;
                text-align: center;

                & li {
                    float: none;
                    display: inline-block;
                    margin-bottom: 14px;
                    margin-right: 4px;

                    & .btn {
                        display: block;
                        height: 32px;
                        line-height: 28px;
                        font-size: 11px;
                        padding: 0 14px;
                        border: 1px solid #e3e4e8;
                        & i {
                            margin-right: 6px;
                            vertical-align: middle;
                        }
                        & span {
                            display: inline-block;
                            vertical-align: middle;
                        }
                    }
                }
            }
        }

        & .desc {
            text-align: center;
            margin-bottom: 90px;

            & p {
                font-family: var(--font4);
                font-size: 14px;
                letter-spacing: -0.7px;
                color: #888888;
                white-space: nowrap;

                & span {
                    margin-right: 3px;
                }
            }
        }
    }
}

/* tablet */
@media (min-width: 767px) {
    .section_expense {
        height: 973px;

        & .inner {
            padding-top: 100px;

            & .txt-area {
                margin-bottom: 54px;

                & .txt-sm {
                    margin-bottom: 6px;
                }

                & .txt-lg {
                    font-size: 36px;
                    line-height: 54px;
                    text-align: center;
                    margin-bottom: 10px;
                    white-space: nowrap;
                }

                & .desc {
                    font-size: 20px;
                    line-height: 29px;
                    letter-spacing: -.5px;
                }
            }

            & .wrap_tabs {
                height: 693px;

                & .panel {
                    & .img {
                        width: 540px;
                    }

                    &:last-of-type {
                        & .img {
                            width: 540px;
                        }
                    }

                    &::before {
                        width: 540px;
                        height: 524px;
                        background-image: url('../../img/common/img_phone_02.png');
                    }

                    &.panel-sm {
                        padding-top: 124px;
                    }
                }

                & .tabs {
                    width: 650px;
                    bottom: 68px;

                    & li {
                        margin-right: 8px;

                        & .btn {
                            height: 42px;
                            line-height: 37.5px;
                            font-size: 15px;
                            padding: 0 17px;
                        }
                    }
                }
            }

            & .desc {
                text-align: center;
                margin-bottom: 90px;

                & p {
                    font-family: var(--font4);
                    font-size: 14px;
                    letter-spacing: -0.7px;
                    color: #888888;
                    white-space: nowrap;

                    & span {
                        margin-right: 3px;
                    }
                }
            }
        }
    }
}

