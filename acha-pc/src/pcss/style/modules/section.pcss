.section {
    position: relative;
}

/*
 s section_main ----------------------------------------
*/
.section_main {
    width: 100%;
    height: 700px;
    background-image: url("../../img/main/img_main_visual_m_768@2x.png");
    background-position: center;
    background-size: cover;
    background-repeat: no-repeat;

    & .inner {
        position: relative;
        width: 100%;
        height: 100%;
        margin: 0 auto;
        padding-top: 60px;

        & .logo {
            display: none;
        }

        & .desc {
            padding-top: 30px;
            margin-bottom: 467px;
            & p {
                font-size: 22px;
                line-height: 30px;
                color: #fff;
                text-align: center;
            }
        }
        & .btn_group {
            position: relative;
            display: block;
            padding: 0 28px;
            z-index: 11;

            & ul {
                display: none;
            }

            & .btn-download-app {
                display: block;
                width: 100%;
                height: 60px;
                line-height: 60px;
                border-radius: 10px;
                background-color: #fcd532;
                font-family: var(--font2);
                font-size: 18px;
                font-weight: 500;
                text-align: center;
            }
            &.up {
                & .btn-download-app {
                    position: fixed;
                    width: 100%;
                    height: 60px;
                    line-height: 60px;
                    bottom: 0;
                    left: 0;
                    border-radius: 0;
                }
            }
        }
    }
}

/* tablet */
@media (min-width: 767px) {
    .section_main {
        height: 800px;
        background-image: url("../../img/main/img_main_visual_m_1024@2x.png");

        & .inner {
            padding-top: 80px;

            & .logo {
                display: none;
            }

            & .desc {
                padding-top: 71px;
                margin-bottom: 445px;
                & p {
                    font-family: var(--font4);
                    font-size: 36px;
                    line-height: 50px;
                }
            }
            & .btn_group {
                padding: 0 40px;

                & ul {
                    display: none;
                }

                & .btn-download-app {
                    height: 80px;
                    line-height: 80px;
                    font-size: 24px;
                }
                &.up {
                    & .btn-download-app {
                        height: 80px;
                        line-height: 80px;
                    }
                }
            }
        }
    }
}

/* pc */
@media (min-width: 1060px) {
    .section_main {
        height: 1000px;
        background-image: url("../../img/main/img_main_visual_2400@2x.png");

        & .inner {
            max-width: 1060px;
            margin: 0 auto;
            padding-top: 347px;

            & .logo {
                display: block;
                margin-bottom: 30px;

                & a {
                    display: inline-block;
                }
            }

            & .desc {
                margin-bottom: 60px;
                padding-top: 30px;
                & p {
                    text-align: left;
                }
            }

            & .btn_group {
                padding: 0;

                & ul {
                    display: block;
                }
                & .btn-download-app {
                    display: none;
                }
            }
        }
    }
}

/*
---------------------------------------------------------
